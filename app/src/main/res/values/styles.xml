<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
    </style>

    <style name="NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
    </style>
    <style name="Ripple_Style_Whait">
        <item name="mrl_rippleOverlay">true</item>
        <item name="mrl_rippleColor">#fff</item>
        <item name="mrl_rippleAlpha">0.2</item>
    </style>

    <style name="Ripple_Style_Blue">
        <item name="mrl_rippleOverlay">true</item>
        <item name="mrl_rippleColor">#ABDBF0</item>
        <item name="mrl_rippleAlpha">0.2</item>
    </style>

    <style name="Ripple_Style_Grey">
        <item name="mrl_rippleOverlay">true</item>
        <item name="mrl_rippleColor">#2196F3</item>
        <item name="mrl_rippleAlpha">0.2</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />



</resources>
